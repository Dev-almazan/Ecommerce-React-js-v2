            \r\n        }\r\n\r\n\r\n        const isInCart = (id) =>\r\n        {\r\n            return cart.some(item =>  item.id === id )\r\n        }\r\n\r\n        const emptyCart =()=>\r\n        {\r\n            setCart([]);\r\n        }\r\n\r\n        const emptyItem =(id)=>\r\n        {\r\n            setCart(cart.filter(item => item.id !== id ))\r\n        }\r\n\r\n\r\n        const totalCart =()=>\r\n        {\r\n        return cart.reduce((accu,item)=> accu + item.price * item.cantidad,0 ) \r\n        }\r\n        \r\n        const totalCant =()=>\r\n        {\r\n        return cart.reduce((accu,item)=> accu +  item.cantidad,0 ) \r\n        }\r\n\r\n        console.log(cart)\r\n\r\n        useEffect(() => {\r\n            localStorage.setItem('cart',JSON.stringify(cart)) \r\n        }, [cart]);\r\n\r\n        return(\r\n            <CartContext.Provider value={\r\n                {\r\n                    cart,\r\n                    addCarrito,\r\n                    isInCart,\r\n                    emptyCart,\r\n                    emptyItem,\r\n                    totalCart,\r\n                    totalCant\r\n                }\r\n            }>\r\n                \r\n                {children}\r\n            </CartContext.Provider>\r\n        )\r\n\r\n    }","C:\\xampp\\htdocs\\desarrollos\\Nueva carpeta\\Ecommerce-React-js-v2\\src\\context\\loginContext.js",["133"],[],"\r\n    import { createContext, useState ,useEffect} from 'react';\r\n\r\n        //Importamos Firebase db\r\n        import {collection, getDocs ,query, where} from \"firebase/firestore\";\r\n        import { db } from \"../firebase/config\";\r\n\r\n    export const LoginContext = createContext();\r\n\r\n\r\n   \r\n\r\n\r\n    \r\n    const usuarios = [\r\n        {\r\n            email: 'ernest_almazan@outlook.com',\r\n            password : 'Dantes98*'\r\n        },\r\n        {\r\n            email: 'pruebas@hotmail.com',\r\n            password : 'Admin2023*'\r\n        }\r\n    ]